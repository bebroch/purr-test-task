{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@token/token/(.*)": "<rootDir>/../libs/token/src/$1",
    "@token/token": "<rootDir>/../libs/token/src",
    "^src/(.*)": "<rootDir>/../src/$1"
  }
}